{
  "name": "iris-client-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:bff": "vue-cli-service serve --port 8081 --mode bff_local",
    "serve:int": "vue-cli-service serve --mode integration",
    "build": "vue-cli-service build --no-module",
    "test:e2e:prepare": "cp .env.e2e_test_local.dist .env.e2e_test_local && cp cypress.env.json.dist cypress.env.json",
    "test:e2e:prepare:win": "copy .env.e2e_test_local.dist .env.e2e_test_local && copy cypress.env.json.dist cypress.env.json",
    "test:e2e": "vue-cli-service test:e2e --mode e2e_test_local",
    "lint": "vue-cli-service lint",
    "generate-licenses": "(license-checker --production --csv --out licenses-prod.csv || true) && license-checker --development --csv --out licenses-dev.csv",
    "convert-licenses": "(csv2md licenses-prod.csv > licenses-prod.md || true) && csv2md licenses-dev.csv > licenses-dev.md"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.1",
    "@mdi/font": "^6.5.95",
    "@types/lodash": "^4.14.177",
    "axios": "^0.24.0",
    "chart.js": "^2.9.4",
    "core-js": "^3.19.1",
    "dayjs": "^1.10.7",
    "json2csv": "^5.0.6",
    "lodash": "^4.17.21",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.3",
    "vuetify": "^2.6.0",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vue/cli-plugin-babel": "~5.0.0-rc.0",
    "@vue/cli-plugin-e2e-cypress": "~5.0.0-rc.0",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.0",
    "@vue/cli-plugin-router": "~5.0.0-rc.0",
    "@vue/cli-plugin-typescript": "~5.0.0-rc.0",
    "@vue/cli-plugin-vuex": "~5.0.0-rc.0",
    "@vue/cli-service": "~5.0.0-rc.0",
    "@vue/eslint-config-typescript": "^9.0.1",
    "@vue/test-utils": "^1.2.2",
    "compression-webpack-plugin": "^9.0.0",
    "csv2md": "^1.1.0",
    "cypress": "^8.7.0",
    "cypress-fail-fast": "^3.2.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.0.2",
    "miragejs": "^0.1.42",
    "prettier": "^2.4.1",
    "sass": "~1.43.4",
    "sass-loader": "^12.3.0",
    "typescript": "~4.4.4",
    "vue-cli-plugin-vuetify": "~2.4.3",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3",
    "webpack": "^5.64.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run lint"
    ]
  }
}
