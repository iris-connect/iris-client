{
  "name": "iris-client-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:bff": "vue-cli-service serve --port 8081 --mode bff_local",
    "serve:int": "vue-cli-service serve --mode integration",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "generate-licenses": "(license-checker --production --csv --out licenses-prod.csv || true) && license-checker --development --csv --out licenses-dev.csv",
    "convert-licenses": "(csv2md licenses-prod.csv > licenses-prod.md || true) && csv2md licenses-dev.csv > licenses-dev.md"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.0",
    "@mdi/font": "^6.1.95",
    "@types/lodash": "^4.14.174",
    "axios": "^0.21.4",
    "chart.js": "^2.9.4",
    "core-js": "^3.18.0",
    "dayjs": "^1.10.7",
    "json2csv": "^5.0.6",
    "lodash": "^4.17.21",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.2",
    "vuetify": "^2.5.8",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-router": "~4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.13",
    "@vue/cli-plugin-vuex": "~4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.2.2",
    "compression-webpack-plugin": "^6.1.1",
    "csv2md": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.18.0",
    "husky": "^6.0.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^11.1.2",
    "miragejs": "^0.1.41",
    "prettier": "^2.4.0",
    "sass": "~1.32.13",
    "sass-loader": "^10.2.0",
    "typescript": "~4.4.3",
    "vue-cli-plugin-vuetify": "~2.4.2",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run lint"
    ]
  }
}
